meta {
  name: Upload CV
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/jobs/:jobId/candidates
  body: multipartForm
  auth: bearer
}

params:path {
  jobId: YOUR_JOB_ID_HERE
}

auth:bearer {
  token: {{token}}
}

body:multipart-form {
  cv: @file(/path/to/your/cv.pdf)
}

tests {
  test("should return 202", function() {
    expect(res.status).to.equal(202);
  });
}
