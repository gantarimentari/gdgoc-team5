meta {
  name: Reprocess Candidate CV
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/candidates/:candidateId/reprocess
  body: multipartForm
  auth: bearer
}

params:path {
  candidateId: YOUR_CANDIDATE_ID_HERE
}

auth:bearer {
  token: {{token}}
}

body:multipart-form {
  cv: @file(/path/to/your/cv.pdf)
}

tests {
  test("should return 202", function() {
    expect(res.status).to.equal(202);
  });
}
